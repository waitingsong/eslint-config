{
  "name": "@waiting/eslint-config",
  "author": "waiting",
  "version": "6.10.0",
  "description": "ESLint Shareable Configuration for Typescrpt",
  "keywords": [
    "ECMAScript Style",
    "code style",
    "eslint",
    "eslintconfig",
    "tslint",
    "typescript"
  ],
  "engines": {
    "node": ">=12.22.0"
  },
  "bin": {},
  "browser": "",
  "es2015": "",
  "main": "index.js",
  "module": "",
  "types": "",
  "bugs": {
    "url": "https://github.com/waitingsong/eslint-config/issues"
  },
  "homepage": "https://github.com/waitingsong/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waitingsong/eslint-config.git"
  },
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "38"
  },
  "devDependencies": {
    "@types/tape": "4",
    "eslint": "8",
    "tape": "5",
    "typescript": ">=4"
  },
  "peerDependencies": {
    "eslint": ">=8"
  },
  "files": [
    "plugins/*.yml",
    "base.js",
    "index.js",
    "recommended.js",
    "strict.js"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint --fix ./*.js --config=strict.js --parser-options=project:tsconfig.json",
    "lint:test": "eslint test/*.ts --config=test/.eslintrc.yml",
    "test": "npm run lint && tape test/*.js",
    "release": "standard-version -r"
  }
}
