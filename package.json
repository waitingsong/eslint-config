{
  "name": "@waiting/eslint-config",
  "author": "waiting",
  "version": "8.9.0",
  "description": "ESLint Shareable Configuration for Typescrpt",
  "keywords": [
    "ECMAScript Style",
    "code style",
    "eslint",
    "eslintconfig",
    "esm",
    "tslint",
    "typescript"
  ],
  "engines": {
    "node": ">=14"
  },
  "type": "module",
  "main": "dist/strict.cjs",
  "exports": {
    ".": {
      "import": "./src/strict.mjs",
      "require": "./dist/strict.cjs"
    },
    "./recommended": {
      "import": "./src/recommended.mjs",
      "require": "./dist/recommended.cjs"
    },
    "./recommended.js": {
      "import": "./src/recommended.mjs",
      "require": "./dist/recommended.cjs"
    },
    "./strict": {
      "import": "./src/strict.mjs",
      "require": "./dist/strict.cjs"
    },
    "./strict.js": {
      "import": "./src/strict.mjs",
      "require": "./dist/strict.cjs"
    },
    "./package.json": "./package.json"
  },
  "bugs": {
    "url": "https://github.com/waitingsong/eslint-config/issues"
  },
  "homepage": "https://github.com/waitingsong/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waitingsong/eslint-config.git"
  },
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^48.0.1"
  },
  "devDependencies": {
    "@types/tape": "4",
    "eslint": "8",
    "tape": "5",
    "typescript": ">=4"
  },
  "peerDependencies": {
    "eslint": ">=8.29.0"
  },
  "files": [
    "plugins/*.yml",
    "src",
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint --fix src/*.mjs dist/*.cjs --config=dist/strict.cjs --parser-options=project:tsconfig.json",
    "lint:test": "eslint test/*.ts --config=test/.eslintrc.yml",
    "test": "npm run lint && tape test/*.js",
    "release": "standard-version -r"
  }
}
